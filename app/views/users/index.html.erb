<div class="container">
  <h3 class="text-center">Users</h3>
  <table class="table">
  	<thead>
      <th> User_id </th>
      <th colspan="3"></th>
  		<th> Email </th>
      <th colspan="3"></th>
      <th> User Name </th>
      <th colspan="3"></th>
      <th> Actions </th>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
        <%= render user %>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
